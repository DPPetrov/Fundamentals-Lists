import java.util.Arrays;
import java.util.List;
import java.util.Scanner;
import java.util.stream.Collectors;

public class SumAdjEqualNums {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        List<Integer> numsList = Arrays.stream(sc.nextLine().split(" ")).map(Integer::parseInt).collect(Collectors.toList());

        for (int i = 0; i < numsList.size() - 1; i++) {
            int sum = 0;
            int first = numsList.get(i);
            int second = numsList.get(i+1);
            if (first == second){
                sum += first + second;
                numsList.set(i, sum);
                numsList.remove(numsList.get(i+1));
                i =-1;
            }

        }

        for(int el : numsList){
            System.out.print(el + " ");
        }
    }
}
